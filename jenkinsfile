pipeline{
    agent any
    stages{

        stage("start"){
            steps{
                echo 'start ci/cd'
                deleteDir()
            }
        }



        stage("maven build"){
            steps{
                    dir("accounts"){
                        sh "mvn clean install"
                    }
                
            }
        }

        stage("maven test"){
            steps{
                 dir("accounts"){

                    sh "mvn test"
                }
                
            }
        }

    }
}